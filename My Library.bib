@article{mwingiraInfluenceLarvalStage2020,
  title = {The {{Influence}} of {{Larval Stage}} and {{Density}} on {{Oviposition Site-Selection Behavior}} of the {{Afrotropical Malaria Mosquito}} {{{\emph{Anopheles}}}}{\emph{ Coluzzii}} ({{Diptera}}: {{Culicidae}})},
  shorttitle = {The {{Influence}} of {{Larval Stage}} and {{Density}} on {{Oviposition Site-Selection Behavior}} of the {{Afrotropical Malaria Mosquito Anopheles}} Coluzzii ({{Diptera}}},
  author = {Mwingira, Victor S and Spitzen, Jeroen and Mboera, Leonard E G and {Torres-Estrada}, Jos{\'e} L and Takken, Willem},
  editor = {Reisen, William},
  year = {2020},
