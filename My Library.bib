@article{gonzalezFlightSwarmingBehaviour2017,
  title = {Flight and Swarming Behaviour of {{{\emph{Culicoides}}}} Species ({{Diptera}}: {{Ceratopogonidae}}) on a Livestock Farm in {{Northern Spain}}},
