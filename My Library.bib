@article{gwynneFemaleMatingSwarms2002,
  title = {Female {{Mating Swarms Increase Predation Risk In A}} '{{Role-Reversed}}' {{Dance Fly}} ({{Diptera}}: {{Empididae}}: {{{\emph{Rhamphomyia Longicauda}}}} {{Loew}})},
  shorttitle = {{{FEMALE MATING SWARMS INCREASE PREDATION RISK IN A}} '{{ROLE-REVERSED}}' {{DANCE FLY}} ({{DIPTERA}}},
  author = {Gwynne, Darryl and Bussi{\`e}re, Luc},
