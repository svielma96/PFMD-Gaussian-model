@article{bode-okeReverseFlightMonarch2020,
  title = {The Reverse Flight of a Monarch Butterfly ( {{{\emph{Danaus}}}}{\emph{ Plexippus}} ) Is Characterized by a Weight-Supporting Upstroke and Postural Changes},
