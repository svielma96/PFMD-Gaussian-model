}

@article{finchMultilevelAnalysisReproduction2020,
  title = {Multi-Level Analysis of Reproduction in an {{Antarctic}} Midge Identifies Female and Male Accessory Gland Products That Are Altered by Larval Stress and Impact Progeny Viability},
  author = {Finch, Geoffrey and Nandyal, Sonya and Perretta, Carlie and Davies, Benjamin and Rosendale, Andrew J. and Holmes, Christopher J. and Gantz, J. D. and Spacht, Drew E. and Bailey, Samuel T. and Chen, Xiaoting and Oyen, Kennan and Didion, Elise M. and Chakraborty, Souvik and Lee, Richard E. and Denlinger, David L. and Matter, Stephen F. and Attardo, Geoffrey M. and Weirauch, Matthew T. and Benoit, Joshua B.},
