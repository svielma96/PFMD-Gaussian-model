  author = {Charlwood, J D and Pinto, J and Sousa, C A and Madsen, H and Ferreira, C},
  year = {2002},
  abstract = {The swarming and mating behaviours of the forest cytoform of Anopheles gambiae s.s. were investigated on 194 evenings and 14 mornings between April 1997 and November 1999 in a peri-urban area of the island of S{\~a}o Tom{\'e}, West Africa. Males swarmed 2 - 3 m above markers of horizontal contrast such as those formed between grass areas and footpaths, or bushes. Evening swarms started 2 min before sunset in sheltered sites and a minute or two later in exposed ones. It took approximately 5 mins from the arrival of the first male for the swarm to reach estimated maximum numbers. Mating pairs were first seen 7 min after the start of swarming. Maximum numbers of pairs in copula were observed 8 min later. Up to 270 pairings were seen in the 20 min period before darkness. Removal of males had no effect on the number of females arriving at the swarm. Males were attracted to sounds that approximated the female flight tone but not to filter paper samples of squashed virgin females swung through the swarm. A much-reduced amount of swarming and a single mating were recorded at dawn. The same locations for swarming were used at different times and at different heights by ants, Culicoides sp. and Culex quinquefasciatus. Journal of Vector Ecology 27(2): 178-183. 2002.},
  langid = {english},
  keywords = {Anopheles,Behavioral,Field experiment,Mosquito},
  file = {/Users/svielma/Zotero/storage/2GCCDDKE/Charlwood et al. - The swarming and mating behaviour of Anopheles gambiae s.s. (Diptera Culicidae) from São Tomé Islan.pdf}
}

