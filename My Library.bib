  shorttitle = {Finding a {{Husband}}},
  author = {Qureshi, Yasser M. and Voloshin, Vitaly and Facchinelli, Luca and McCall, Philip J. and Chervova, Olga and Towers, Cathy E. and Covington, James A. and Towers, David P.},
