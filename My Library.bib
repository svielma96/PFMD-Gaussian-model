@misc{fialaNuptialFlightSoutheast2017,
  title = {Nuptial Flight of the {{Southeast Asian}} Plant-Ant {{{\emph{Crematogaster}}}}{\emph{ Captiosa}} ({{Forel}}, 1911) and the Phenology of Colony Founding},
